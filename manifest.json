{
    "name": "Syncthing",
    "id": "syncthing",
    "packaging_format": 1,
    "description": {
        "en": "Continuous file synchronization program",
        "fr": "Programme de synchronisation de fichiers en continu"
    },
    "version": "1.18.5~ynh1",
    "url": "https://syncthing.net/",
    "upstream": {
        "license": "MPL-2.0",
        "website": "https://syncthing.net",
        "userdoc": "https://docs.syncthing.net",
        "code": "https://github.com/syncthing/syncthing"
    },
    "license": "MPL-2.0",
    "maintainer": [
        {
            "name": "txmrl",
            "email": "txmrl@txmrl.net"
        },
        {
            "name": "yalh76"
        }
    ],
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/syncthing",
                "default": "/syncthing"
            },
            {
                "name": "admin",
                "type": "user"
            }
        ]
    }
}
